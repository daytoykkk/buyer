/*搜索框*/
.search_box{
    width: 100%;
    height: 122rpx;
    background-color: #3a4058;
    padding-left: 26rpx;
    padding-right: 30rpx;
    display: flex;
    align-items: center;
}
.search_input{
    width: 614rpx;
	height: 64rpx;
	background-color: #f8f8f8;
    border-radius: 36rpx;
    display: flex;
    align-items: center;
    padding-left: 32rpx;
    padding-right: 14rpx;
}
.icon_cancel{
    margin-left: auto;
    margin-right: 0;
}
.cancel_text{
    font-family: FZLanTingHei-R-GBK;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 34rpx;
	letter-spacing: 0rpx;
    color: #ffffff;
    margin-left: auto;
    margin-right: 0;
}

/*商品*/
.good_item{
    width: 100%;
    height: 208rpx;
    margin-top: 32rpx;
    border-bottom: solid 2rpx #f8f8f8;
    display: flex;
    align-items: center;
}
.img{
    width: 168rpx;
    height: 168rpx;
    margin-left: 32rpx;
    overflow: hidden;
    background-color: #ffffff;
}
.img image{
    width: 168rpx;
    height: 168rpx;
}
.good_msg{
    width: 484rpx;
    height: 168rpx;
    margin-left: 32rpx;
}
.name{
    font-family: FZLanTingHei-R-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 26rpx;
	letter-spacing: 0rpx;
    color: #000000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
}
.price_cart{
    width: 100%;
    height: 44rpx;
    display: flex;
    margin-top: 100rpx;
}
.price{
    font-family: FZLanTingHei-B-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 26rpx;
	letter-spacing: 0rpx;
	color: #f7937b;
}