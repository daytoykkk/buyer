.person_info {
    width: 100%;
    height: 250rpx;
    padding-top: 20rpx;
    background-image: linear-gradient(180deg, #3a4058 0%, #3a4058 100%);
    position: relative;
}
.person_info:after {
      background-color: #3a4058;
      content: '';
      width: 116%;
      height: 380rpx;
      position: absolute;
      left: -8%;
      top:0;
      z-index: -1;
      border-radius: 0 0 80% 80%;
}
.selfmsg_box{
    width: 92%;
    height: 116rpx;
    margin-left: 4%;
    display: flex;
    align-items: center;
}
.tx{
    width: 116rpx;
	height: 116rpx;
	background-image: linear-gradient(90deg, 
		);
    border: solid 2rpx #ffffff;
    border-radius: 50%;
    overflow: hidden;
}
.tx image{
    width: 116rpx;
    height: 116rpx;
}
.msg{
    margin-left: 32rpx;
}
.msg_name{
    font-family: FZLanTingHei-R-GBK;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 30rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
}
.msg_id{
    font-family: FZLanTingHei-R-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 24rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
}

/*优惠券*/
.quan_box{
    width:91%;
    height: 206rpx;
    margin-left:4%;
    margin-top: -70rpx;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
    overflow:hidden; 
}
.quan{
    clear: both;
    width: 100%;
    height: 150rpx;
    background-image: linear-gradient(180deg, #8d8eab 0%, #8d8eab 100%);
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}
.quan::after{
    background-color: #8d8eab;
    content: '';
    width: 116%;
    height: 206rpx;
    position: absolute;
    left: -8%;
    top:0;
    z-index: -1;
    border-radius: 0 0 80% 80%;
    box-shadow: 0rpx 6rpx 12rpx 0rpx 
		rgba(0, 0, 0, 0.16);
}
.quan_text{
    margin-top: 50rpx;
    margin-left: 16rpx;
    font-family: FZLanTingHei-R-GBK;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #ffffff;
}

/*订单导航*/
.daohang{
    width: 100%;
    height: 58rpx;
    font-family: FZLanTingHei-R-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0rpx;
