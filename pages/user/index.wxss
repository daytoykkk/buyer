.person_info {
    width: 100%;
    height: 180rpx;
    padding-top: 20rpx;
    background-image: linear-gradient(180deg, #3a4058 0%, #3a4058 100%);
}
.selfmsg_box{
    width: 92%;
    height: 116rpx;
    margin-left: 4%;
    display: flex;
    align-items: center;
}
.tx{
    width: 116rpx;
	height: 116rpx;
	background-image: linear-gradient(90deg, 
		);
    border: solid 2rpx #ffffff;
    border-radius: 50%;
    overflow: hidden;
}
.tx image{
    width: 116rpx;
    height: 116rpx;
}
.msg{
    margin-left: 32rpx;
}
.msg_name{
    font-family: FZLanTingHei-R-GBK;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 30rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
}
.msg_id{
    font-family: FZLanTingHei-R-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 24rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
}

/*优惠券*/
.quan_box{
    width:91%;
    height: 206rpx;
    margin-left:4%;
    margin-top: -70rpx;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
    overflow:hidden; 
}
.quan{
    width: 94%;
    margin-left: 3%;
    margin-top: 24rpx;
    height: 110rpx;
    background-image: linear-gradient(180deg, #8d8eab 0%, #8d8eab 100%);
    box-shadow: 0rpx 8rpx 4rpx hsla(0, 3%, 87%, 0.902);
    border-radius: 12rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.quan_text{
    margin-left: 16rpx;
    font-family: FZLanTingHei-R-GBK;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #ffffff;
}

/*订单导航*/
.daohang{
    width: 100%;
    height: 58rpx;
    font-family: FZLanTingHei-R-GBK;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0rpx;
    color: #000000;
    display: flex;
}
.daohang_now{
    width: 50%;
}
.daohang_all{
    width: 50%;
}
.daohang_line{
    width: 100%;
	height: 10rpx;
	background-color: #8d8eab;
}
.daohang_active{
    color: #6d6d6d;
    background-color: #f8f8f8;
}


.list_box{
    margin-top: 20rpx;
    height: 60rpx;
    width: 100;
    background-color: #ffffff;
    position: relative;
}
.list_msg{
    width: 96%;
    margin-left: 4%;
    font-size: 30rpx;
    display: flex;
    align-content: center;
}
.select_box{
    background-color: #fefeff;
    border-radius: 4rpx;
    border-left: 1rpx solid #ccc;
    border-right: 1rpx solid #ccc;
    width: 150rpx;
    padding: 0 10rpx;
    position: absolute;
    top: 60rpx;
    left: 32rpx;
    z-index:1;
    overflow: hidden;
    animation: myfirst 0.5s;
}
@keyframes myfirst{
    form{
        height: 0rpx;
    }
    to{
        height:60rpx
    }
}
.select_one{
    height: 50rpx;
    font-size: 30rpx;
    line-height: 40rpx;
    align-items: center;
    border-bottom: 1rpx solid #ccc;
}

